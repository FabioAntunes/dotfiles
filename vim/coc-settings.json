{
  "diagnostic.errorSign": "✖",
  "diagnostic.warningSign": "⚑",
  "diagnostic.infoSign": "⚑",
  "diagnostic.hintSign": "⚑",
  "languageserver": {
    "golang": {
      "command": "gopls",
      "rootPatterns": [
        "go.mod",
        ".vim/",
        ".git/",
        ".hg/"
      ],
      "filetypes": [
        "go"
      ],
      "initializationOptions": {
        "usePlaceholders": true
      }
    }
  }
}
